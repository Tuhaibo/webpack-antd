{
  "name": "webpack-react-antd",
  "version": "1.0.0",
  "description": "webpack-react-antd",
  "main": "index.js",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie < 11",
    "iOS >= 6",
    "Android >= 4"
  ],
  "scripts": {
    "dev": "NODE_ENV=development webpack --config ./webpack.dev.config.js",
    "server": "NODE_ENV=production webpack-dev-server --config ./webpack.dev.config.js",
    "build": "NODE_ENV=production webpack --config ./webpack.prod.config.js --color --progress",
    "build:dll": "webpack --config ./webpack.dll.config.js",
    "start": "npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tuhaibo/webpack-antd.git"
  },
  "keywords": [
    "webpack-react-antd"
  ],
  "author": "haibo.tu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Tuhaibo/webpack-antd/issues"
  },
  "homepage": "https://github.com/Tuhaibo/webpack-antd#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.2",
    "html-webpack-plugin": "^4.2.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.4",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  }
}
